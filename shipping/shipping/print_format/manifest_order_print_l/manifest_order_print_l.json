 "html": "<style>\r\n  .shipping-label {\r\n    font-family: Arial, sans-serif;\r\n    width: 100%;\r\n    max-width: 480px;\r\n    border: 1px solid #000;\r\n  }\r\n  .header {\r\n    display: flex;\r\n    border-bottom: 1px solid #000;\r\n  }\r\n  .header-left {\r\n    /*flex: 0 0 25%;*/\r\n    flex-basis: 25%;\r\n    font-size: 42px;\r\n    font-weight: bold;\r\n    border-right: 1px solid #000;\r\n    padding: 10px;\r\n    position: relative;\r\n  }\r\n  .header-right {\r\n    flex: 1;\r\n    height: 101px;\r\n    padding: 10px;\r\n    font-size: 10px;\r\n  }\r\n  .barcode {\r\n    padding: 50px;\r\n    text-align: right;\r\n    transform: translateY(-70px);\r\n  }\r\n  .service-type {\r\n    border-bottom: 1px solid #000;\r\n    padding: 8px;\r\n    font-weight: bold;\r\n    text-align: center;\r\n    font-size: 14px;\r\n  }\r\n  .addresses {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    border-bottom: 1px solid #000;\r\n    padding: 10px;\r\n  }\r\n  .details-section {\r\n    flex: 1;\r\n    margin-right: 20px;\r\n    background-color: #f8f9fa; /* Light background color */\r\n    border: 1px solid #ddd; /* Subtle border */\r\n    border-radius: 8px; /* Rounded corners */\r\n    padding: 15px; /* Inner spacing */\r\n    font-size: 14px;\r\n  }\r\n  .details-section div {\r\n    display: flex;\r\n    flex-direction: column; /* Stack label and data vertically */\r\n    margin-bottom: 10px; /* Gap under each record */\r\n  }\r\n  .details-section strong {\r\n    font-weight: bold;\r\n    color: #333; /* Darker text for labels */\r\n    margin-bottom: 5px; /* Space between label and data */\r\n  }\r\n  .details-section span {\r\n    color: #555; /* Slightly lighter text for values */\r\n  }\r\n  .location-box {\r\n    flex: 1;\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    justify-content: center; /* Center content vertically */\r\n    font-size: 14px;\r\n    border: 2px solid #000; /* Bold border */\r\n    border-radius: 10px; /* Rounded corners */\r\n    padding: 20px 10px 10px 10px; /* Add padding-top (20px) and other padding */\r\n    background-color: #fff; /* White background */\r\n    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1); /* Subtle shadow */\r\n  }\r\n  .location-box div {\r\n    margin: 5px 0;\r\n  }\r\n  .origin, .destination {\r\n    font-weight: bold;\r\n    font-size: 20px; /* Larger font size */\r\n    text-transform: uppercase; /* Uppercase text */\r\n  }\r\n  .arrow {\r\n    font-size: 28px; /* Larger arrow */\r\n    font-weight: bold; /* Bold arrow */\r\n    margin: 10px 0; /* Spacing around the arrow */\r\n  }\r\n  .recipient {\r\n    font-size: 12px;\r\n  }\r\n  .recipient-title {\r\n    font-weight: bold;\r\n  }\r\n  .bottom-barcode {\r\n    text-align: center;\r\n    border-bottom: 1px solid #000;\r\n    padding: 10px 0;\r\n  }\r\n  .tracking-number {\r\n    text-align: center;\r\n    padding: 5px;\r\n    font-size: 12px;\r\n    border-bottom: 1px solid #000;\r\n  }\r\n  .qr-code {\r\n    text-align: right;\r\n    padding: 5px;\r\n  }\r\n  .shipment-details {\r\n    padding: 10px;\r\n    font-size: 12px;\r\n  }\r\n  .shipment-details table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n  }\r\n  .shipment-details th,\r\n  .shipment-details td {\r\n    padding: 8px;\r\n    text-align: left;\r\n    font-size: 12px;\r\n    border-bottom: 1px solid #ddd;\r\n  }\r\n  .shipment-details th {\r\n    background-color: #f8f9fa;\r\n    font-weight: bold;\r\n    color: #333;\r\n  }\r\n  .shipment-details tr:hover {\r\n    background-color: #f1f1f1;\r\n  }\r\n</style>\r\n\r\n<div style=\"width: 100%; height: 100%; display: flex; align-items: center; justify-content: center;\">\r\n  <div class=\"shipping-label\">\r\n    <div class=\"header\">\r\n      <div class=\"header-left\">\r\n        <!--<p style=\"font-size: 70px;display: flex;align-items: center;justify-content: center;\">G</p>-->\r\n        <img src=\"/assets/erpnext/images/cal_logo2.png\" alt=\"Barcode\" style=\"max-width: 100%; height:auto;\">\r\n      </div>\r\n      <div class=\"header-right\">\r\n        {{ doc.date }}<br>\r\n        <div class=\"barcode\">\r\n          <img src=\"{{ doc.qr_code }}\" alt=\"Barcode\" style=\"max-width: 40%; height:auto;\">\r\n          \r\n        </div>\r\n        \r\n      </div>\r\n    </div>\r\n\r\n    <!--<div class=\"service-type\">-->\r\n    <!--  {{ doc.consilidation_cal }}-->\r\n    <!--</div>-->\r\n\r\n    <div class=\"addresses\">\r\n      <div class=\"details-section\">\r\n        <div>\r\n          <strong>Date:</strong>\r\n          <span>{{ doc.date }}</span>\r\n        </div>\r\n        <div>\r\n          <strong>Airline AWB Number:</strong>\r\n          <span>{{ doc.airline_awb_number }}</span>\r\n        </div>\r\n        <!--<div>-->\r\n        <!--  <strong>Consolidation CAL:</strong>-->\r\n        <!--  <span>{{ doc.consilidation_cal }}</span>-->\r\n        <!--</div>-->\r\n        <div>\r\n          <strong>Total Weight:</strong>\r\n          <span>{{ doc.total_weight }}</span>\r\n        </div>\r\n        <div>\r\n          <strong>Total No. of Pieces:</strong>\r\n          <span>{{ doc.total_no_of_pieces }}</span>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"location-box\">\r\n        <div>Origin</div>\r\n        <div class=\"origin highlight\">{{ doc.port_of_origin }}</div>\r\n        <div class=\"arrow\">\u2193</div>\r\n        <div class=\"destination highlight\">{{ doc.port_of_destination }}</div>\r\n        <div>Destination</div>\r\n      </div>\r\n    </div>\r\n\r\n    <div class=\"service-type\" style=\"border-top: 1px solid #000;\">\r\n      Shipment Details\r\n    </div>\r\n\r\n    <div class=\"shipment-details\">\r\n      <table>\r\n        <thead>\r\n          <tr>\r\n            <th>Pieces Number</th>\r\n            <th>CAL AWB</th>\r\n            <th>Weight</th>\r\n            <th>Length</th>\r\n            <th>Width</th>\r\n            <th>Height</th>\r\n          </tr>\r\n        </thead>\r\n        <tbody>\r\n          {% for item in doc.shipment_details %}\r\n          <tr>\r\n            <td>{{ item.pieces_number }}</td>\r\n            <td>{{ item.cal_awb }}</td>\r\n            <td>{{ item.weight }}</td>\r\n            <td>{{ item.length }}</td>\r\n            <td>{{ item.width }}</td>\r\n            <td>{{ item.height }}</td>\r\n          </tr>\r\n          {% endfor %}\r\n        </tbody>\r\n      </table>\r\n    </div>\r\n\r\n    <div class=\"bottom-barcode\">\r\n      <img src=\"{{ doc.bottom_barcode_url|default('https://t4.ftcdn.net/jpg/02/28/23/91/360_F_228239110_4eEmhcqbUpZG8y1x1aazFBQMVmbGjoce.jpg') }}\" alt=\"Bottom Barcode\" style=\"max-width: 66%;\">\r\n    </div>\r\n\r\n    <div class=\"tracking-number\">\r\n      {{ doc.airline_awb_number }}\r\n    </div>\r\n  </div>\r\n</div>",
