 "html": "<!DOCTYPE html>\n<html>\n<head>\n    <title>Cargo Print Format</title>\n    <style>\n        body {\n            font-family: Arial, sans-serif;\n            font-size: 12px;\n            margin: 0;\n            padding: 0;\n            display: flex;\n            justify-content: center;\n            align-items: center;\n            height: 100vh;\n        }\n        .page-container {\n            display: flex;\n            justify-content: center;\n            padding: 10px;\n        }\n        .label-container {\n            width: 12cm;\n            background: #fff;\n            border: 2px solid #000;\n            padding: 10px;\n            box-sizing: border-box;\n            display: flex;\n            flex-direction: column;\n            align-items: center;\n        }\n        .header {\n            display: flex;\n            justify-content: space-between;\n            align-items: center;\n            width: 100%;\n        }\n        .logo-img {\n            width: 100px;\n        }\n        .qrcode-img {\n            width: 100px;\n        }\n        .air-waybill, .air-waybill-row {\n            border-bottom: 2px solid #000;\n            padding: 3px 0;\n            width: 100%;\n            display: flex;\n            justify-content: space-between;\n        }\n        .txt {\n            font-weight: 400;\n            font-size: 10px;\n            margin: 0;\n        }\n        .awb-number, .origin-destination, .pieces-weight {\n            font-weight: 600;\n            font-size: 12px;\n            text-align: center;\n            margin: 0;\n        }\n        .origin, .destination, .pieces, .weight {\n            width: 48%;\n            padding: 3px;\n            box-sizing: border-box;\n        }\n        .origin {\n            border-right: 2px solid #000;\n        }\n        .table {\n            width: 100%;\n            border-collapse: collapse;\n            font-size: 8px;\n            margin-top: 3px;\n        }\n        .table th, .table td {\n            border: 1px solid #000;\n            padding: 2px;\n            text-align: center;\n        }\n        .table th {\n            background-color: #f2f2f2;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"page-container\">\n        <div class=\"label-container\">\n            <div class=\"header\">\n                <img src=\"/assets/erpnext/images/logo22.jpg\" class=\"logo-img\">\n                <img src=\"{{ doc.qr_code }}\" class=\"qrcode-img\">\n            </div>\n            <div class=\"air-waybill\">\n                <p class=\"txt\">Air Waybill No.</p>\n                <p class=\"awb-number\">{{ doc.airline_awb_numbber }}</p>\n            </div>\n            <div class=\"air-waybill-row\">\n                <div class=\"origin\">\n                    <p class=\"txt\">Origin</p>\n                    <p class=\"origin-destination\">{{ doc.port_of_origin }}</p>\n                </div>\n                <div class=\"destination\">\n                    <p class=\"txt\">Destination</p>\n                    <p class=\"origin-destination\">{{ doc.port_of_destination }}</p>\n                </div>\n            </div>\n            <div class=\"air-waybill-row\">\n                <div class=\"pieces\">\n                    <p class=\"txt\">Total No. Pieces</p>\n                    <p class=\"pieces-weight\">{{ doc.total_no_of_pieces }}</p>\n                </div>\n                <div class=\"weight\">\n                    <p class=\"txt\">Total Weight</p>\n                    <p class=\"pieces-weight\">{{ doc.total_weight }}</p>\n                </div>\n            </div>\n            <table class=\"table\">\n                <thead>\n                    <tr>\n                        <th>Pieces</th>\n                        <th>AWB</th>\n                        <th>Weight</th>\n                        <th>L</th>\n                        <th>W</th>\n                        <th>H</th>\n                    </tr>\n                </thead>\n                <tbody>\n                    {% for item in doc.shipment_details %}\n                    <tr>\n                        <td>{{ item.pieces_number }}</td>\n                        <td>{{ item.cal_awb }}</td>\n                        <td>{{ item.weight }}</td>\n                        <td>{{ item.length }}</td>\n                        <td>{{ item.width }}</td>\n                        <td>{{ item.height }}</td>\n                    </tr>\n                    {% endfor %}\n                </tbody>\n            </table>\n        </div>\n    </div>\n</body>\n</html>",
